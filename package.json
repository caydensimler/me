{
  "name": "ics-base",
  "author": "Image Conscious Studios",
  "contributors": [
    {
      "name": "Marin Carroll",
      "email": "marin@marincarroll.com"
    }
  ],
  "private": true,
  "license": "UNLICENSED",
  "homepage": "https://github.com/icscreative/ics-base",
  "description": "A blank canvas to build gorgeous themes upon.",
  "engines": {
    "node": ">=16 <17"
  },
  "scripts": {
    "start": "wp-scripts start",
    "build": "wp-scripts build",
    "lint:style": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "rtl": "rtlcss assets/css/style.css assets/css/style-rtl.css",
    "bundle": "dir-archiver --src . --dest ../ics-base.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules package-lock.json package.json phpcs.xml.dist sass"
  },
  "devDependencies": {
    "@wordpress/babel-preset-default": "^6.5.0",
    "@wordpress/block-editor": "^8.1.0",
    "@wordpress/server-side-render": "^3.1.0",
    "@wordpress/scripts": "^20.0.2",
    "autoprefixer": "^10.4.2",
    "sass-math": "^1.0.0",
    "dir-archiver": "^1.2.0",
    "mini-css-extract-plugin": "^2.4.6",
    "no-emit-webpack-plugin": "^4.0.1",
    "rtlcss": "^3.5.0",
    "sass": "^1.47.0",
    "stylelint-config-wordpress": "^17.0.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  }
}
